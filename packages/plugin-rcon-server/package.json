{
  "name": "@cinderlink/plugin-rcon-server",
  "description": "RCON server plugin for Cinderlink",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.0-next.0",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./dist/index.js",
  "types": "src/index.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.cjs",
      "types": "./src/index.ts"
    }
  },
  "scripts": {
    "watch": "bun run build.ts --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/identifiers": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "multiformats": "^13.3.7"
  },
  "dependencies": {
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/identifiers": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "@cinderlink/test-adapters": "workspace:*",
    "@cinderlink/tsconfig": "workspace:*",
    "@types/ws": "^8.5.4",
    "typescript": "^5.8.3"
  }
}
