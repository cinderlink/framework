{
  "name": "@cinderlink/plugin-rcon-server",
  "description": "RCON server plugin for Cinderlink",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.0-next.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/identifiers": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "multiformats": "^11.0.2"
  },
  "dependencies": {
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/identifiers": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "@cinderlink/test-adapters": "workspace:*",
    "@cinderlink/tsconfig": "workspace:*",
    "@types/ws": "^8.5.4",
    "tsup": "^6.7.0",
    "typescript": "^5.8.3",
    "vitest": "^0.30.1"
  }
}
