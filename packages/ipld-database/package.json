{
  "name": "@candor/ipld-database",
  "description": "IPLD database for @candor/client",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "1.0.0-rc.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts  --external dids,multiformats",
    "watch": "pnpm build --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@candor/core-types": "workspace:../core-types",
    "@candor/test-adapters": "workspace:../test-adapters",
    "@types/ajv": "^1.0.0",
    "@types/node": "^18.13.0",
    "dids": "^3.4.0",
    "multiformats": "^11.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.5"
  },
  "peerDependencies": {
    "dids": "^3.4.0",
    "multiformats": "^11.0.1"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "emittery": "^1.0.1",
    "minisearch": "^6.0.1"
  }
}
