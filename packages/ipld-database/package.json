{
  "name": "@candor/ipld-database",
  "description": "IPLD database for @candor/client",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "1.0.0-rc.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts  --external dids,multiformats",
    "watch": "pnpm build --watch",
    "test": "vitest"
  },
  "devDependencies": {
    "@candor/client": "workspace:../client",
    "@candor/core-types": "workspace:../core-types",
    "@types/ajv": "^1.0.0",
    "dids": "^3.4.0",
    "multiformats": "^10.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.25.8"
  },
  "peerDependencies": {
    "dids": "^3.4.0",
    "multiformats": "^10.0.3"
  },
  "dependencies": {
    "ajv": "^8.11.2",
    "emittery": "^1.0.1",
    "minisearch": "^6.0.0"
  }
}
