{
  "name": "@cinderlink/plugin-identity-server",
  "description": "Identity server plugin for Cinderlink",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.0-next.0",
  "type": "module",
  "main": "./dist/index.js",
