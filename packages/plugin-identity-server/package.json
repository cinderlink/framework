    "@cinderlink/test-adapters": "workspace:*",
    "@cinderlink/tsconfig": "workspace:*",
    "@ipld/car": "^5.1.1",
    "@types/node": "^18.16.6",
    "it-all": "^3.0.1",
    "multiformats": "^13.3.7",
    "typescript": "^5.8.3",
    "viem": "^2.21.58",
