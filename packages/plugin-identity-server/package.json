{
  "name": "@candor/plugin-identity-server",
  "private": true,
  "description": "Identity server plugin for Candor",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "1.0.0-rc.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts ",
    "watch": "pnpm build --watch"
  },
  "dependencies": {
    "@candor/client": "workspace:../client",
    "@candor/core-types": "workspace:../core-types",
    "@candor/ipld-database": "workspace:../ipld-database"
  },
  "devDependencies": {
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  }
}
