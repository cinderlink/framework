{
  "name": "@cryptids/plugin-dkg-server",
  "description": "Distributed key generation server plugin for Cryptids",
  "authors": "Drew <drew@cryptids.xyz>",
  "version": "1.0.0-rc.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "pnpm build --watch"
  },
  "dependencies": {
    "@cryptids/client": "link:../client",
    "@cryptids/ipld-database": "link:../ipld-database"
  },
  "devDependencies": {
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.25.8"
  }
}
