{
  "name": "@cinderlink/server-bin",
  "description": "Cinderlink server binary",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.0-next.0",
  "main": "dist/bin.js",
  "type": "module",
  "module": "./dist/bin.js",
  "types": "./dist/bin.d.ts",
  "exports": {
    ".": {
      "types": "./dist/bin.d.ts",
      "import": "./dist/bin.js",
      "require": "./dist/bin.cjs"
    }
  },
  "bin": {
    "cinderlink": "dist/cinderlink"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "watch": "bun run build.ts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cinderlink/identifiers": "workspace:*",
    "@cinderlink/plugin-identity-server": "workspace:*",
    "@cinderlink/plugin-offline-sync-server": "workspace:*",
    "@cinderlink/plugin-social-server": "workspace:*",
    "@cinderlink/protocol": "workspace:*",
    "@cinderlink/server": "workspace:*",
    "chalk": "^5.2.0",
    "dotenv": "^16.5.0",
    "@helia/http": "^2.1.2",
    "minimist": "^1.2.8",
    "multiformats": "^13.3.7",
    "viem": "^2.21.58"
  },
  "peerDependencies": {
    "@cinderlink/client": "workspace:*"
  },
  "devDependencies": {
    "@cinderlink/plugin-social-core": "workspace:*",
    "@cinderlink/plugin-sync-db": "workspace:*",
    "@cinderlink/tsconfig": "workspace:*",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.16.6",
    "make-error": "^1.3.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "vitest": "^0.30.1",
    "yn": "^5.0.0"
  }
}
