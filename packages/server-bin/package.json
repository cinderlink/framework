{
  "name": "@cinderlink/server-bin",
  "description": "Cinderlink server binary",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.0.1",
  "main": "dist/bin.js",
  "type": "module",
  "module": "./dist/bin.js",
  "types": "./dist/bin.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bin.js",
      "require": "./dist/bin.cjs",
      "types": "./dist/bin.d.ts"
    }
  },
  "bin": {
    "cinderlink": "dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@cinderlink/server": "workspace:0.0.1",
    "ipfs-http-gateway": "^0.13.0",
    "ipfs-http-server": "^0.15.0",
    "minimist": "^1.2.8"
  },
  "peerDependencies": {
    "@cinderlink/client": "workspace:^0.0.1"
  },
  "devDependencies": {
    "@cinderlink/identifiers": "workspace:^0.0.1",
    "@cinderlink/plugin-identity-server": "workspace:^0.0.1",
    "@cinderlink/plugin-offline-sync-server": "workspace:^0.0.1",
    "@cinderlink/plugin-social-server": "workspace:^0.0.1",
    "@cinderlink/protocol": "workspace:^0.0.1",
    "@cinderlink/tsconfig": "workspace:^1.0.0",
    "@types/minimist": "^1.2.2",
    "chalk": "^5.2.0",
    "ethers": "^5.7.2",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vitest": "^0.28.5"
  }
}
