{
  "name": "@cinderlink/plugin-offline-sync-server",
  "description": "Offline messaging sync server plugin for Cinderlink",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.0-next.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "watch": "bun run build.ts --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cinderlink/plugin-offline-sync-core": "workspace:*"
  },
  "devDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/plugin-offline-sync-client": "workspace:*",
    "@cinderlink/protocol": "workspace:*",
    "@cinderlink/tsconfig": "workspace:*",
    "did-jwt": "^8.0.17",
    "dids": "^5.0.3",
    "typescript": "^5.8.3",
    "vitest": "^0.30.1",
    "bun-dts": "^0.1.70"
  },
  "peerDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/plugin-offline-sync-client": "workspace:*",
    "@cinderlink/plugin-offline-sync-core": "workspace:*",
    "@cinderlink/protocol": "workspace:*",
    "did-jwt": "^8.0.17",
    "dids": "^5.0.3"
  }
}
