{
  "name": "@candor/plugin-sync-db",
  "description": "Database sync plugin for the Candor framework",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "devDependencies": {
    "@candor/core-types": "workspace:^0.1.2",
    "@candor/ipld-database": "workspace:^0.1.2",
    "@candor/tsconfig": "workspace:^1.0.0",
    "@types/node": "^18.14.2",
    "@types/uuid": "^9.0.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "vitest": "^0.29.2"
  }
}
