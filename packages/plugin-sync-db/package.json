{
  "name": "@cinderlink/plugin-sync-db",
  "description": "Database sync plugin for the Cinderlink framework",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.1-next.0",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.cjs",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "watch": "bun run build.ts --watch",
    "test": "bun test",
    "test:integration": "bun test src/plugin.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/identifiers": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "@cinderlink/schema-registry": "workspace:*",
    "@cinderlink/test-adapters": "workspace:*",
    "@cinderlink/tsconfig": "workspace:*",
    "@ipld/car": "^5.1.1",
    "@types/node": "^18.16.6",
    "@types/uuid": "^9.0.1",
    "dids": "^5.0.3",
    "emittery": "^1.0.1",
    "it-all": "^3.0.1",
    "multiformats": "^13.3.7",
    "typescript": "^5.8.3",
    "uuid": "^9.0.0",
    "viem": "^2.21.58"
  },
  "peerDependencies": {
    "@cinderlink/identifiers": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "@cinderlink/test-adapters": "workspace:*",
    "emittery": "^1.0.1",
    "multiformats": "^13.3.7"
  }
}
