    "@libp2p/utils": "^6.6.5",
    "@multiformats/multiaddr": "^12.4.0",
    "did-jwt": "^8.0.4",
    "dids": "^5.0.3",
    "emittery": "^1.0.3",
    "helia": "^5.4.2",
