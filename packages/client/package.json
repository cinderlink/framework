    "@libp2p/peer-id-factory": "^2.0.3",
    "@libp2p/websockets": "^6.0.0",
    "@multiformats/multiaddr": "^12.1.2",
    "@stablelib/ed25519": "^1.0.3",
    "@types/ajv": "^1.0.0",
    "@types/lodash-es": "^4.17.7",
