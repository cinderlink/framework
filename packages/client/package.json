    "dotenv": "^16.0.3",
    "ethers": "^6.3.0",
    "ipfs-core-types": "^0.14.0",
    "it-all": "^3.0.1",
    "it-pushable": "^3.1.3",
    "it-to-buffer": "^4.0.1",
    "libp2p": "^0.44.0",
    "lodash-es": "^4.17.21",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
