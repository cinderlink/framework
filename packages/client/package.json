{
  "name": "@candor/client",
  "description": "Typescript client for the Cinderlink development toolkit",
  "authors": "Drew Ewing <drew@cinderlink.com>",
  "version": "0.0.1-rc.1",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "rm -rf dist/ && tsup",
    "watch": "pnpm build --watch --",
    "watch:browser": "vite build --watch",
    "build:browser": "vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@chainsafe/libp2p-gossipsub": "^6.1.0",
    "@chainsafe/libp2p-noise": "^11.0.0",
    "@libp2p/crypto": "^1.0.11",
    "@libp2p/interface-connection": "^3.0.8",
    "@libp2p/interface-peer-id": "^2.0.1",
    "@libp2p/peer-id": "^2.0.1",
    "@libp2p/peer-id-factory": "^2.0.1",
    "@libp2p/websockets": "^5.0.3",
    "@multiformats/multiaddr": "^11.4.0",
    "@stablelib/ed25519": "^1.0.3",
    "@types/ajv": "^1.0.0",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "dotenv": "^16.0.3",
    "ipfs-core-types": "^0.14.0",
    "it-pushable": "^3.1.2",
    "libp2p": "^0.42.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.28.4"
  },
  "peerDependencies": {
    "@chainsafe/libp2p-gossipsub": "^5.3.0",
    "@chainsafe/libp2p-noise": "^10.2.0",
    "@libp2p/crypto": "^1.0.10",
    "@libp2p/peer-id": "^1.1.18",
    "@libp2p/peer-id-factory": "^1.0.20",
    "@libp2p/websockets": "^5.0.2",
    "@multiformats/multiaddr": "^11.1.4",
    "@stablelib/ed25519": "^1.0.3",
    "libp2p": "^0.41.0"
  },
  "dependencies": {
    "@candor/core-types": "workspace:../core-types",
    "@candor/ipld-database": "workspace:../ipld-database",
    "@libp2p/mplex": "^7.1.1",
    "ajv": "^8.12.0",
    "did-jwt": "^6.11.0",
    "did-resolver": "^4.0.1",
    "dids": "^3.4.0",
    "emittery": "^1.0.1",
    "ipfs-core": "^0.18.0",
    "it-length-prefixed": "^8.0.4",
    "it-map": "^2.0.0",
    "it-pipe": "^2.0.5",
    "key-did-provider-ed25519": "^2.0.1",
    "key-did-resolver": "^2.3.0",
    "localforage": "^1.10.0",
    "microbundle": "^0.15.1",
    "multiformats": "^11.0.1",
    "uint8arrays": "^4.0.3",
    "uuid": "^9.0.0"
  }
