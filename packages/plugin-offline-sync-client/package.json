{
  "name": "@cinderlink/plugin-offline-sync-client",
  "description": "Offline sync client plugin for cinderlink enabling users to send encrypted messages to offline users and have them delivered when they come online.",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.0-next.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "@cinderlink/plugin-offline-sync-core": "workspace:*",
    "@cinderlink/tsconfig": "workspace:*",
    "@libp2p/interface-connection": "^5.1.1",
    "@multiformats/multiaddr": "^12.5.1",
    "@types/uuid": "^9.0.1",
    "did-jwt": "^8.0.17",
    "dids": "^5.0.3",
    "emittery": "^1.0.1",
    "multiformats": "^13.3.7",
    "tsup": "^6.7.0",
    "typescript": "^5.8.3",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "@cinderlink/core-types": "workspace:*",
    "@cinderlink/ipld-database": "workspace:*",
    "@cinderlink/protocol": "workspace:*",
    "@multiformats/multiaddr": "^11.4.0",
    "did-jwt": "^8.0.17",
    "dids": "^5.0.3",
    "emittery": "^1.0.1",
    "multiformats": "^13.3.7"
  },
  "dependencies": {
    "@cinderlink/protocol": "workspace:*",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.0"
  }
}
