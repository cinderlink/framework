{
  "name": "@candor/plugin-offline-sync-client",
  "description": "Offline sync client plugin for candor enabling users to send encrypted messages to offline users and have them delivered when they come online.",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm ",
    "watch": "pnpm build --watch"
  },
  "devDependencies": {
    "@candor/core-types": "workspace:../core-types",
    "@candor/ipld-database": "workspace:../ipld-database",
    "@candor/plugin-offline-sync-core": "workspace:../plugin-offline-sync-core",
    "@multiformats/multiaddr": "^11.4.0",
    "@types/uuid": "^9.0.0",
    "did-jwt": "^6.11.1",
    "dids": "^3.4.0",
    "emittery": "^1.0.1",
    "multiformats": "^11.0.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "peerDependencies": {
    "@multiformats/multiaddr": "^11.4.0",
    "did-jwt": "^6.11.0",
    "dids": "^3.4.0",
    "emittery": "^1.0.1",
    "multiformats": "^11.0.1"
  },
  "dependencies": {
    "date-fns": "^2.29.3",
    "uuid": "^9.0.0"
  }
}
