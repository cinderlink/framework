{
  "name": "@cinderlink/schema-registry",
  "description": "Schema registry with versioning and migration support for Cinderlink",
  "authors": "Drew <drew@cinderlink.com>",
  "version": "1.0.0-next.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "watch": "bun run build.ts --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "@cinderlink/tsconfig": "workspace:*",
    "@types/node": "^18.19.107",
    "typescript": "^5.8.3",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "zod": "^4.0.5"
  },
  "types": "dist/index.d.ts"
}
