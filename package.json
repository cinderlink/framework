{
  "name": "@cinderlink/framework",
  "private": true,
  "version": "0.0.3",
  "description": "",
  "type": "module",
  "main": "index.js",
  "packageManager": "bun@1.2.17",
  "workspaces": [
    "./packages/*",
    "./examples/*",
    "./apps/*"
  ],
  "scripts": {
    "watch": "turbo watch --filter='./packages/*' --parallel --concurrency 24",
    "build": "turbo build --filter='./packages/*' --concurrency 2",
    "dev": "turbo dev",
    "test": "bun --filter=./packages/* -r test",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "typecheck": "turbo typecheck --filter='./packages/*'",
    "prepare": "husky",
    "docs:build": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "keywords": [],
  "author": "Drew Ewing",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@eslint/js": "^9.28.0",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "chokidar": "^3.5.3",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "minimist": "^1.2.8",
    "multiformats": "^13.3.7",
    "oxlint": "^1.6.0",
    "tshy": "^3.0.2",
    "turbo": "^2.5.4",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "viem": "^2.31.6"
  }
}
