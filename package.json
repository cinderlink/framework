{
  "name": "@cinderlink/framework",
  "private": true,
  "version": "0.0.3",
  "description": "",
  "type": "module",
  "main": "index.js",
  "packageManager": "bun@1.2.17",
  "workspaces": [
    "./packages/*",
    "./examples/*",
    "./apps/*"
  ],
  "scripts": {
    "watch": "bun --filter='./packages/*' run watch",
    "build": "bun run build.ts",
    "build:old": "bun --filter='./packages/*' run build",
    "dev": "bun --filter='./examples/*' --filter='./apps/*' run dev",
    "test": "bun --filter='./packages/*' run test",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "typecheck": "bun --filter='./packages/*' run typecheck",
    "prepare": "husky",
    "docs:build": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "keywords": [],
  "author": "Drew Ewing",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@eslint/js": "^9.28.0",
    "@types/bun": "^1.2.17",
    "@types/jsdom": "^21.1.7",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "chokidar": "^3.5.3",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.0",
    "minimist": "^1.2.8",
    "multiformats": "^13.3.7",
    "oxc-transform": "^0.77.0",
    "oxlint": "^1.6.0",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "viem": "^2.31.6"
  }
}
