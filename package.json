{
  "name": "@cinderlink/framework",
  "private": true,
  "version": "0.0.3",
  "description": "",
  "type": "module",
  "main": "index.js",
  "packageManager": "bun@1.2.17",
  "workspaces": [
    "./packages/*",
    "./examples/*",
    "./apps/*"
  ],
  "scripts": {
    "watch": "bun run build.ts --watch",
    "build": "bun run build.ts",
    "build:bin": "bun run build-bin.ts",
    "build:all": "bun run build && bun run build:bin",
    "dev": "bun --filter='./examples/*' --filter='./apps/*' run dev",
    "test": "bun test",
    "test:packages": "bun test packages",
    "test:integration": "bun test tests/integration",
    "test:e2e": "bun test tests/e2e",
    "dev:plugins": "bun run scripts/dev/plugin-dev-server.ts",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:stop": "docker-compose stop",
    "docker:start": "docker-compose start",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:primary": "docker-compose logs -f primary-node",
    "docker:logs:secondary": "docker-compose logs -f secondary-node",
    "docker:reset": "docker-compose down -v && rm -rf data/primary-node data/secondary-node && docker-compose up -d",
    "docker:reset:data": "rm -rf data/primary-node data/secondary-node",
    "docker:clean": "docker-compose down -v && docker-compose rm -f",
    "docker:test": "docker-compose -f docker-compose.test.yml run --rm test-runner bun test:packages",
    "docker:test:client": "docker-compose -f docker-compose.test.yml run --rm test-runner bun test packages/client",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "typecheck": "bun --filter='./packages/*' run typecheck",
    "prepare": "husky",
    "docs:build": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "keywords": [],
  "author": "Drew Ewing",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@eslint/js": "^9.28.0",
    "@types/bun": "^1.2.17",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "multiformats": "^13.3.7",
    "oxlint": "^1.36.0",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.7.0"
  },
  "dependencies": {
    "viem": "^2.31.6"
  },
  "overrides": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  }
}
