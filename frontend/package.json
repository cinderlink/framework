{
	"name": "frontend",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@cryptids/client": "workspace:../packages/client",
		"@cryptids/interface-plugin": "workspace:../packages/interface-plugin",
		"@cryptids/plugin-social-client": "workspace:../packages/plugin-social-client",
		"@esbuild-plugins/node-globals-polyfill": "^0.1.1",
		"@esbuild-plugins/node-modules-polyfill": "^0.1.4",
		"@iconify-json/logos": "^1.1.19",
		"@iconify-json/mdi": "^1.1.38",
		"@libp2p/peer-id": "^1.1.18",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-static": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@sveltejs/package": "^1.0.0",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"@unocss/reset": "^0.47.6",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"multiformats": "^10.0.2",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"unocss": "^0.47.6",
		"vite": "^4.0.1",
		"vitest": "^0.25.8"
	}
}
