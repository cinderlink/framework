{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./packages/client/src/index.ts",
    "./packages/server/src/index.ts",
    "./packages/core-types/src/index.ts",
    "./packages/identifiers/src/index.ts",
    "./packages/ipld-database/src/index.ts",
    "./packages/protocol/src/index.ts",
    "./packages/schema-registry/src/index.ts"
  ],
  "entryPointStrategy": "resolve",
  "out": "./docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "githubPages": false,
  "readme": "README.md",
  "navigationLinks": {
    "Documentation": "/docs",
    "GitHub": "https://github.com/cinderlink/framework"
  },
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Client",
    "Server",
    "Core Types",
    "Database",
    "Identity",
    "Protocol",
    "Plugins"
  ],
  "groupOrder": [
    "Classes",
    "Interfaces",
    "Type Aliases",
    "Functions",
    "Variables"
  ],
  "tsconfig": "./tsconfig.json",
  "hideGenerator": true,
  "customCss": "./docs/api-style.css"
}